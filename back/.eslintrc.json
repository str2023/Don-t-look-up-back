{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-use-before-define": [
      // function 호이스팅 에러 제거
      "error",
      { "functions": false, "classes": true, "variables": true }
    ],
    "no-console": "off", //console.log warning 제거
    "no-unused-vars": ["error", { "args": "none" }] //사용되지 않는 매개변수 에러 제거
  }
}
